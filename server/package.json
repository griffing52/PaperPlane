    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/supertest": "^6.0.3",
    "commander": "^14.0.2",
    "supertest": "^7.1.4",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "zod": "^4.1.12"
  }
}
